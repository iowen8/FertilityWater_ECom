<div class="page contact-page">
    <div class="page-body">
  
<!-- Title & BreadCrumbs -->
<section class="mtop">
	<section class="container-fluid container">
		<section class="row-fluid">
			<section id="donation_box">
				<section class="container container-fluid">
					<section class="row-fluid">
					<div class="span8 first"> <h2> Contact Us</h2> </div>
				
		
		</section>
				</section>
			</section>		<!-- end of Page Title -->
	</section>
 		<section class="row-fluid">
			<!-- BreadCrumbs -->
				<figure id="breadcrumbs" class="span12">
					<ul class="breadcrumb">
					<li><a href="@Request.Url.GetLeftPart(UriPartial.Authority)">Home</a> <span class="divider">/</span></li>
					<li class="active">Contact Us </li>
					</ul>
				</figure>
			<!-- End of breadcrumbs -->
		</section>
	</section>
</section>
<!-- End of Tile & Breadcrumbs -->

<!-- Page Content Container -->
<section id="content" class="mbtm contact_us">
	<section class="container-fluid container">
	
		<section class="row-fluid">
			
			<figure class="span12 mbtm2" id="map_holder"> 		<iframe class="map-border" src="https://www.google.com/maps/embed/v1/place?q=120%20E%20State%20Rd%20434%2C%20Winter%20Springs%2C%20FL%2032708%2C%20United%20States&key=AIzaSyA6DccYXRHmkPiEcwYn7YJSUpGjgic2T7w"></iframe> </figure>
			
 
		</section>
	</section>
    <script>
        function contactFormSubmit()
        {
            var cfName = $("#CFname").val();
            var cfEmail = $("#CFemail").val();
            var cfComments = $("#CFcomments").val();
            //todo validate
            $.ajax({
                cache: false,
                type: "POST",
                url: "@Url.Action("SendMessage", "Contact")",
                data: { name:  cfName, email : cfEmail, comments:cfComments },
            success: function (data) {
                alert("success");
                alert(data.success);
            },
            error: function (data) {
                alert('Failed.');
            }
        });
        }
    </script>
	<section class="container-fluid container">
			<section class="row-fluid">
			
			<section id="contact_form" class="span6 form">
			<h3> Contact Form </h3>
			<p> Thank you for visiting our website. Please fill out the following form to request information about our products or to provide feedback about our site. When you are finished, click the ‘Submit’ button to send us your message. </p>
			
			<form method="post" action="#" onsubmit="return false;">
                <input type="text" value="Enter Your Name" name="CFname" id="CFname" required />
                <input type="email" value="Email Address" name="CFemail" id="CFemail" required />
                <textarea name="CFcomments" id="CFcomments" cols="10" rows="10" required> Enter your Comment</textarea>
                <input type="button" class="btns" value="Submit" name="CFsubmit" id="CFsubmit" onclick="contactFormSubmit();" />
			</form>
			</section>
		
			<section id="contact_info" class="span6 contact_info">
			<h3> Contact Information </h3>
			
			<figure class="span12 first"> <i class="icon-envelope-alt "></i>info@myfertilitypharmacy.com</figure>
			
			<figure class="span12 first"> <i class="icon-mobile-phone"></i>(407) 557-2029</figure>
				
				<figure class="span12 first"> 
				<div class="span6 fisrt">
				<i class="icon-map-marker"></i> 
                    120 E State Rd 434 <br /><span style="margin-left:20px;">&nbsp;</span>Winter Springs, FL 32708
 				</div>
				</figure>
 

				<figure id="n_social" class="span12 first"> 
				
					<a href="#"> <i class="icon-facebook"></i> </a> 
					<a href="#"> <i class="icon-twitter"></i> </a> 
					<a href="#"> <i class="icon-google-plus"></i> </a> 
					<a href="#"> <i class="icon-linkedin"></i> </a> 
					<a href="#"> <i class="icon-pinterest"></i> </a> 
					
				
				</figure>
				
			</section>
		
			</section>
		</section>
	
	</section>

 <!-- Page Content Container -->
	 
